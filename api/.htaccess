# .htaccess
<IfModule mod_rewrite.c>
    RewriteEngine On
    
    # Redirect fonts directory access to actual files
    RewriteRule ^fonts/(.*)$ fonts/$1 [L]
    
    # API endpoints
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule ^(.*)$ api/index.php [QSA,L]
</IfModule>